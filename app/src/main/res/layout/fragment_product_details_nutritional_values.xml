<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProductDetailsNutritionalValuesFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".presentation.InfoActivity">


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/col_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintEnd_toStartOf="parent"
            app:layout_constraintGuide_end="100dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/col_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintEnd_toStartOf="@id/col_end"
            app:layout_constraintGuide_end="200dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/row"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="80dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/row_bottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:orientation="horizontal"
            app:layout_constraintGuide_begin="110dp" />

        <fr.leprohon.labs.esgi_kotlin.utils.TableCellView
            android:id="@+id/tableCellView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:padding="5dp"
            android:text="@string/i00g"
            android:textAlignment="center"
            android:textStyle="bold"
            app:autoSizeTextType="uniform"
            app:border="left|bottom"
            app:layout_constraintBottom_toTopOf="@+id/row"
            app:layout_constraintEnd_toStartOf="@+id/col_end"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/col_1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <fr.leprohon.labs.esgi_kotlin.utils.TableCellView
            android:id="@+id/tableCellView2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:padding="5dp"
